{
  "page": {
    "config": {
      "adsenseAttributes": {
        "page_url": "https://www.google.com"
      },
      "categoryExclusion": ["airline"],
      "collapseDiv": "BEFORE_FETCH",
      "disableInitialLoad": true,
      "lazyLoad": {
        "fetchMarginPercent": 300,
        "mobileScaling": 2.0,
        "renderMarginPercent": 100
      },
      "privacyTreatments": {
        "treatments": ["disablePersonalization"]
      },
      "safeFrame": {
        "forceSafeFrame": true
      },
      "singleRequest": true
    }
  },
  "slots": [
    {
      "adUnit": "/123/abc",
      "size": [300, 250],
      "config": {
        "targeting": {"slot": "blue"}
      }
    }
  ]
}
